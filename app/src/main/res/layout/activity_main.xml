<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/turnToAccessibilitySetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转到辅助服务设置" />

    <Button
        android:id="@+id/startWechat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="启动企业微信" />

    <Button
        android:id="@+id/sendDataToService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="发送数据到Service" />

    <Button
        android:id="@+id/dialogPermission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开悬浮窗设置" />

    <Button
        android:id="@+id/closeDialogPermission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭悬浮窗设置" />

</LinearLayout>